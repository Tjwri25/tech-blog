

  {{#if projects.length}}
  <div class="col-md-6 project-list">
    <h3>Your Posts:</h3>

    {{#each projects as |project|}}
    <div class="row mb-2 ">
      <div class="col-md-8">
        <h4><a class = "projname" href="/project/{{project.id}}">{{project.name}}</a></h4>
        <p class="post">
      {{project.description}}
      </p>
      </div>
      <div class="col-md-4 centerme">
        <i class="btn btn-sm btn-primary fas fa-trash-alt" data-id="{{project.id}}"></i>
      </div>
       
    </div>
   
    {{/each}}
  </div>
  {{/if}}
</div>

<script src="./js/profile.js"></script>

